local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Whale Scripts | Slap Battles", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = " [ Commands ] ",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddLabel(" [ This Script Was Coded By @WhaleRoblox On YT ]")
Tab:AddLabel(" [ Free Abilitys You Can Use Without Any Slaps ]")

Tab:AddButton({
	Name = "Rhythm Explosion",
	Callback = function()
      		game:GetService("ReplicatedStorage").rhythmevent:FireServer("AoeExplosion",0)
            
  	end    
})

Tab:AddButton({
	Name = "Null Ability",
	Callback = function()
      		game:GetService("ReplicatedStorage").NullAbility:FireServer() 
            
  	end    
})

Tab:AddLabel(" [ Slap Require Gloves ]")

Tab:AddButton({
	Name = "Diamond",
	Callback = function()
      		fireclickdetector(workspace.Lobby.Diamond.ClickDetector)
            wait(0.01)
game:GetService("ReplicatedStorage"):WaitForChild("Rockmode"):FireServer()
      
  	end    
})
